            index_header.style.cssText = 'width:' + (_width * 2) + 'px;height:' + (_height - headerHeight - (FloatHeight == 0 ? 50 : (FloatHeight + 2))) + 'px';
           // setTimeout(function () {
                myscroll = new iScroll("page_scroll_0", {
                    vScroll: true,
                    vScrollbar: false,
                    vScrollbar: false,
                    bounce: false,
                    lockDirection: true,
