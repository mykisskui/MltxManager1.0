body {

    margin:0;
    padding:0;
    font:14px/1.5;
    font-family:'Microsoft YaHei';
    max-width:640px;
    margin:auto;

}
.on {
    display:none;
}

/*
        中
    */
.indexbuttonHeader {
    position:relative;
    height:34px;
    padding:0 1px;
}
.indexbutton_ed {/*切换*/
            position:absolute;
            top:0;
            left:0;
}
.index_default {
    position:relative;
}
.index_header {
  position:absolute;
  top:0;
  left:0;

}
.index_1, .index_0 {
    float:left;
    
}
.index_1 {
    left:500px;
}


/*核心end*/
/*首页头部start*/

.index_0_header_top {
    background-color:#fff;
}
.index_0_header_top > ul {
      list-style-type:none;
      margin:0;
      padding:0;
      display:-webkit-box;
      line-height:2.2;
      border-bottom:1px solid #cccccc;
      font-size:1.5em;
}
    .index_0_header_top > ul > li {
        -webkit-box-flex:1;
        text-align:center;
    }
        .index_0_header_top > ul > li{
            width:10%;
        }
    .index_0_header_top > ul > li {
            color:#83b99c;
    }
            .index_0_header_top > ul > li:nth-child(2) {
                width:60%;
                line-height:1;
            }
                .index_0_header_top > ul > li:nth-child(2) > div {
                    display:inline-table;
                    line-height:2;
                    margin:10px 0;
                    background-color:#64a786;
                    color:#fff;
                    border-radius:5px;
                }
.indexbutton {
    font-size: 16px;
    float: left;
    width:80px;
    height:32px;
}
.indexbutton_off {
    color:#fff;
}
.indexbutton_on {
    background-color:#fff;
    margin:1px;
    color:#64a786;
    border-radius:5px;
}

/*首页头部end*/



@media screen and (max-width:320px ) {/*当屏幕尺寸小于>320*/
           /*首页幻灯片0 start*/
            .index_0_slide_div {
                min-height:100px;
               background-color:#ccc;
            }
            /*首页幻灯片0 end*/


            /*              首页菜单 start*/
.index_0_Menu_div > ul {
        list-style-type:none;
        padding:0;
        margin:5px 10px;
        display:inline-table;
        text-align:center;
     
}
    .index_0_Menu_div > ul > li {
        float:left;
        width:25%;
        margin:5px 0;
    }
.index_0_Menu_div > ul > li > div {

}
.index_0_Menu_div > ul > li > div  > img{
            width:50px;height:50px;
            border-radius:15px;
}
    .index_0_Menu_div > ul > li > div > div {
        margin:2px 0;
        color:#999;
        font-size:13px;
    }
/*首页菜单 end*/



/*首页模块 start*/
.index_0_Module {
    margin:0 10px 0 10px;
}
.index_0_Module_1 {
    width:100%;
    height:100px;
    }
    .index_0_ModuleMargin {
        margin:5px 0 0 0;
    }
    .index_0_Module_3 {
        width:100%;
        height:100px;
    }
        .index_0_Module_3 > ul {
            list-style-type:none;
            margin:0;padding:0;
            display:-webkit-box;
            height:100%;
        }
            .index_0_Module_3 > ul > li {
                -webkit-box-flex:1;
                border-left:2px solid #eee;
            }
                .index_0_Module_3 > ul > li:nth-child(1) {
                    border:none;
                }
    .index_0_Module_4 {
        width:100%;
        height:170px;
        margin-bottom:5px;
    }
        .index_0_Module_4 > ul {
            list-style-type:none;
            margin:0;padding:0;
            display:block;
        }
            .index_0_Module_4 > ul > li:nth-child(1) {
                        width:100%;
                        height:70px;
                        border-bottom:1px solid #eee;
            }
            .index_0_Module_4 > ul > li:nth-child(2) {
                width:45%;
                height:100px;
                float:left;
                border:1px solid #eee;
                -webkit-box-sizing:border-box;
            }
            .index_0_Module_4 > ul > li:nth-child(3) {
                width:55%;
                height:50px;
                float:left;
                        border:1px solid #eee;
                -webkit-box-sizing:border-box;
            }
            .index_0_Module_4 > ul > li:nth-child(4) {
                width:55%;
                height:50px;
                float:left;
                        border:1px solid #eee;
                -webkit-box-sizing:border-box;
            }
            
/*首页模块 end*/




/*首页浮动菜单start*/

    .index_0_FloatMenu {
        height:50px;
        width:100%;
        background-color:rgba(255,255,255,1);
        border-top:1px solid #d6d6d6;
     
        }
        .index_0_FloatMenu > ul {
            list-style-type:none;
            margin:0;
            padding:0;
            display:block;
        }
            .index_0_FloatMenu > ul > li {
                width:20%;
                float:left;
                height:100%;
            }
                .index_0_FloatMenu > ul > li > div {
                    margin:6px auto 2px;
                    height:22px;
                }
                .index_0_FloatMenu > ul > li > div >  img {
                      width:100%;
                      height:100%;
                         max-width:28px;
                         max-height:23px;
                         min-height:23px;
                         display:block;
                         margin:auto;
                }
                .index_0_FloatMenu > ul > li > span {
                    height:20px;
                    font-size:14px;
                    text-align:center;
                    color:#737373;
                    display:block;
                }

/*首页浮动菜单end*/






}/*小于320结束*/


